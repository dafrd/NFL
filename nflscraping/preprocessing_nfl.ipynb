    "\n",
    "#   - Reseting the index\n",
    "df = df.reset_index(drop=True)\n",
    "\n",
    "\n",
    "#   - Deletion of useless variables\n",
    "del drop_lines\n",
    "\n",
    "#   - Clean file creation for analysis\n",
    "\n",
    "os.makedirs('working_files', exist_ok=True)  \n",
    "df.to_csv('working_files/scores_prep.csv')  \n",
    "\n",
    "df"
